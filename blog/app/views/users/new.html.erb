<h1>New User</h1>


<%= form_for @user do |f|   %>
	<%= f.label :first_name %>
	<%= f.text_field :first_name, autofocus: true  %>
	<%= f.label :last_name  %>
	<%= f.text_field :last_name  %>
	<%= f.label :email  %>
	<%= f.email_field :email %>
	<%= f.label :password %>
	<%= f.password_field :password %>
	<%= f.submit %>

<% end %>


<%= link_to 'Existing User', sessions_new_path %>