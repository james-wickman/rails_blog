 <h1><%= @current_user.first_name  %>
 <div>
	 <%= @current_user.last_name  %>
	 </h1>
	 <button><%= link_to 'edit', edit_user_path(@current_user) %></button>
	 <button><%= link_to 'Delete', user_path(@current_user.id), method: :delete %></button>
	 <button><%= link_to 'New Post', new_post_path %></button>
</div>
<hr>
<% @posts.each do |post|  %>
 	<hr>
	<%= post.content %>
	<br>
	<button><%= link_to 'edit Post', edit_post_path(post.id) %></button>
	<button><%= link_to 'Comments', post_path(id: post.id) %> </button>
	<hr>
<% end %>